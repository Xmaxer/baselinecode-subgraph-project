{
  "name": "@baselinecode/subgraph-project",
  "version": "1.0.5",
  "keywords": [
    "node",
    "template",
    "baselinecode"
  ],
  "repository": {
    "url": "https://github.com/Xmaxer/baselinecode-subgraph-project"
  },
  "license": "MIT",
  "author": "Kevin Jakubauskas",
  "type": "commonjs",
  "bin": {
    "@baselinecode/subgraph-project": "./main.js"
  },
  "scripts": {
    "build": "tsx build.mts",
    "lint": "eslint . --max-warnings 0 --fix",
    "lint:ci": "eslint . --max-warnings 0",
    "prepare": "husky",
    "prettier": "prettier --write .",
    "prettier:ci": "prettier --check .",
    "publish:npm": "cd dist && npm publish",
    "test": "rm -rf test-project && tsx src/main.mts -n test-project",
    "typescript:check": "tsc --project tsconfig.esbuild.json && tsc --project tsconfig.project.json",
    "version-bump:major": "npm version major --commit-hooks false",
    "version-bump:minor": "npm version minor --commit-hooks false",
    "version-bump:patch": "npm version patch --commit-hooks false"
  },
  "dependencies": {
    "commander": "14.0.2",
    "cross-spawn": "7.0.6",
    "yauzl": "3.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@trivago/prettier-plugin-sort-imports": "6.0.0",
    "@types/cross-spawn": "6.0.6",
    "@types/node": "24.10.4",
    "@types/yauzl": "2.10.3",
    "@typescript-eslint/eslint-plugin": "8.50.1",
    "@typescript-eslint/parser": "8.50.1",
    "esbuild": "0.27.2",
    "eslint": "9.39.2",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "9.1.7",
    "prettier": "3.7.4",
    "prettier-plugin-packagejson": "2.5.20",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=24",
    "npm": "*",
    "yarn": "Use npm"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
